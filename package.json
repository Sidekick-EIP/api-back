{
  "name": "back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:staging": "dotenv -e .env.staging -- nest start",
    "start:prod": "dotenv -e .env.production -- nest start",
    "db:push": "npx prisma db push",
    "db:push:staging": "dotenv -e .env.staging -- npx prisma db push",
    "db:push:prod": "dotenv -e .env.production -- npx prisma db push",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:deploy:staging": "dotenv -e .env.staging -- npx prisma migrate deploy",
    "migrate:deploy:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "migrate:staging": "dotenv -e .env.staging -- npx prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate dev",
    "migrate:staging:reset": "dotenv -e .env.staging -- npx prisma migrate reset",
    "migrate:prod:reset": "dotenv -e .env.production -- npx prisma migrate reset",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:sync": "jest --runInBand",
    "test:verbose": "jest --verbose",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:aws": "dotenv -e .env.production -- npm i && npx prisma generate && npm run build && node dist/main.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.289.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/websockets": "^9.3.9",
    "@prisma/client": "^4.5.0",
    "amazon-cognito-identity-js": "^5.2.10",
    "argon2": "^0.28.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-cli": "^5.1.0",
    "jwks-rsa": "^2.1.4",
    "multer-s3": "^3.0.1",
    "passport": "^0.5.3",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^16.11.32",
    "@types/passport-jwt": "^3.0.6",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^4.8.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
